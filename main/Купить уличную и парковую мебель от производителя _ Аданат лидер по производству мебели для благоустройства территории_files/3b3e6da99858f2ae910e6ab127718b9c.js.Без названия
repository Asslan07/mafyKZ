$.checkQuantity($input);},checkQuantity:function($obj){var $input=$obj,$block=$input.closest('.quantity'),$less=$block.find('span.less'),$more=$block.find('span.more');if($input.val()==1){$less.addClass('disabled');}
else{$less.removeClass('disabled');}},buy:function($button){var $btn=$button,$block=$btn.closest('.actions'),$dataID=$btn.data('quantity'),$input=$block.find('#'+$dataID),$quantity=$input.val(),$itemID=$btn.next('input').val(),$headerCart=$('#headerCart');$btn.attr('disabled','disabled');$.ajax({url:CART,type:'POST',data:{'buy':$itemID,'quantity':$quantity},dataType:'json',async:true,success:function($answer){$headerCart.html($answer);},complete:function(){$btn.removeAttr('disabled');}});},getModificationID:function($obj){var $block=$obj,$itemID=$obj.data('id'),$btn=$('#buy_'+$itemID),$params=$('#params_'+$itemID),$good=$('#good_'+$itemID),$imagesContainer=$good.find('.image'),$props=new Array();$obj.find('input:checked.prop').each(function(){var $item={};$item[$(this).data('id')]=$(this).val();$props.push($item);});$.ajax({url:SHOP,type:'POST',data:{'get_modification':$itemID,'props':$props},dataType:'json',async:true,success:function($answer){var $answer=$('<div>').html($answer),$item=$answer.find('#item'),$buyID=$item.data('id'),$itemSKU=$item.data('marking'),$paramsInfo=$item.children('#options'),$images=$item.children('#images'),$draws=$item.children('#draws'),$types=$item.children('#types'),$files=$item.children('#files'),$labels=$item.children('#labels');if($('#debug').length){$('#debug').html($answer);}
$params.html($paramsInfo);$imagesContainer.html($images.children());$imagesContainer.append($draws.children());$('#types_'+$itemID).html($types.children());$('#files_'+$itemID).html($files.children());$imagesContainer.append($labels.children());$('#buy_'+$itemID).attr('data-id',$buyID).change();$('#input_buy_'+$itemID).val($buyID).change();$('#sku').html($itemSKU).change();},complete:function(){$good.find('.swiper').each(function(){var $sliderID=$('#'+$(this).attr('id'));$.slider($sliderID);});$.lightbox();}});},chooseConfig:function($btn,$postfix){var $btn=$btn,$containerID=$btn.data('target'),$container=$('#'+$containerID),$postfix=$postfix,$label=$('label#label_'+$postfix),$radio=$('input[type="radio"]#radio_'+$postfix),$checked=$('input[type="hidden"]#checked_'+$postfix),$checkedID=$checked.val(),$list=$('input[type="hidden"]#list_'+$postfix),$listID=$list.val(),$items=$('input[type="hidden"]#items_'+$postfix),$itemsIDs=$items.val();$.ajax({url:SHOP,type:'POST',data:{'advanced':'ajax','list':$listID,'items':$itemsIDs,'checked':$checkedID,'postfix':$postfix},dataType:'json',async:true,success:function($answer){$container.html($answer);},complete:function(){$container.attr('data-view','show');$container.on('click','.close',function(){$container.attr('data-view','hide');setTimeout(()=>{$container.empty();},250);}).on('click','.col[data-postfix]',function(){var $obj=$(this),$id=$obj.data('id'),$name=$obj.data('name'),$postfix=$obj.data('postfix');$('input[type="radio"]#radio_'+$postfix).val($id).change();$('input[type="hidden"]#checked_'+$postfix).val($id);$('label#label_'+$postfix).html($name);$container.attr('data-view','hide');setTimeout(()=>{$container.empty();},250);});}});},slider:function($obj){var $object=$obj,$sliderID='#'+$object.attr('id');var $swiper=new Swiper($sliderID+' .swiper-box',{navigation:{nextEl:$sliderID+' .next',prevEl:$sliderID+' .prev',},speed:300,spaceBetween:30,parallax:false,loop:false,autoHeight:true,effect:"slide",grabCursor:true,preloadImages:false,a11y:false,lazy:true,lazy:{loadPrevNext:true,loadPrevNextAmount:1,}});},unknownDownloads:function(){$('.download').on('click','.files a[data-ajax="must-auth"]',function(){$.authModal();$.when($.authModal()).done(function(){console.log("modal",'try message');setTimeout(()=>{var $target=$('#modal').find('div.error'),$message='<p>Для скачивания файлов необходимо авторизоваться!</p>';$target.html($message);$target.attr('data-view','show');},300);});});}});;
